{
  "name": "@lehelkovach/knowshowgo-client",
  "version": "0.2.1",
  "description": "JavaScript and Python client SDKs for KnowShowGo semantic memory engine",
  "main": "src/index.js",
  "types": "src/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lehelkovach/knowshowgo-client.git"
  },
  "homepage": "https://github.com/lehelkovach/knowshowgo-client",
  "bugs": {
    "url": "https://github.com/lehelkovach/knowshowgo-client/issues"
  },
  "keywords": [
    "semantic-memory",
    "knowledge-graph",
    "ai-agents",
    "hallucination-detection",
    "knowshowgo"
  ],
  "author": "Leh Kovach",
  "license": "MIT",
  "peerDependencies": {
    "knowshowgo": "^0.2.1"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "esbuild": "^0.17.0"
  },
  "scripts": {
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage",
    "build": "esbuild src/index.js --bundle --platform=node --outfile=dist/index.cjs",
    "prepack": "npm run build && npm run test",
    "prepublishOnly": "npm run test && npm run test:integration"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
